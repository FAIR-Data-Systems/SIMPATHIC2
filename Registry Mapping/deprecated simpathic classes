    <!-- https://w3id.org/nmd-domain#SIMPATHIC_00001 -->

    <owl:Class rdf:about="https://w3id.org/nmd-domain#SIMPATHIC_00001">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NCIT_C20993"/>
        <dc:description xml:lang="en">Hammersmith Functional Motor Scale; A scale used to gather information on disease progression and motor abilities in patients with Spinal Muscular Atrophy (types 2 and 3).</dc:description>
        <rdfs:label xml:lang="en">HFMS-Score</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/nmd-domain#SIMPATHIC_00002 -->

    <owl:Class rdf:about="https://w3id.org/nmd-domain#SIMPATHIC_00002">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NCIT_C20993"/>
        <dc:description xml:lang="en">Hammersmith Functional Motor Scale Expanded; A clinician-reported outcome measure that expands the original HFMS to capture higher functioning in SMA patients using 13 additional items adapted from the gross motor function measure (GMFM), scored on a three-point scale. A higher score indicates a higher level of function.</dc:description>
        <rdfs:label xml:lang="en">HFMS-E-Score</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/nmd-domain#SIMPATHIC_00003 -->

    <owl:Class rdf:about="https://w3id.org/nmd-domain#SIMPATHIC_00003">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/NCIT_C20993"/>
        <dc:description xml:lang="en">Hammersmith Infant Neurological Examination (HINE) Section 2; A standardized neurological examination for identifying infants between 3 and 24 months of age who have a high probability of all types of cerebral palsy (CP). It includes 26 items assessing cranial nerves, posture, movements, tone, and reflexes.</dc:description>
        <rdfs:label xml:lang="en">HINE-Section2-Score</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/nmd-domain#SIMPATHIC_00004 -->

    <owl:Class rdf:about="https://w3id.org/nmd-domain#SIMPATHIC_00004">
